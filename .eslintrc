{
  "rules": {
    "eqeqeq": [2, "smart"],
    "indent": [
      2,
      2,
      { "SwitchCase": 1 }
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ]
  },
  "env": {
    "es6": true,
    "browser": true,
    "commonjs": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
  }
}
